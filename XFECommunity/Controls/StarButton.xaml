<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="XCCChatRoom.Controls.StarButton">
    <Grid>
        <ImageButton
            Source="unstar"
            x:Name="starButton"
            Aspect="Center"
            Clicked="starButton_Clicked"/>
        <Image
            Source="starcircle"
            x:Name="circleImage"
            Aspect="Center"
            IsVisible="False"/>
    </Grid>
</ContentView>
